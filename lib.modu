fn internalFredFinder(name) {
    if name == "Fred" {
        return "Your name is Fred! How cool!";
    }
    if name != "Fred" {
        return "Your name is not Fred. How sad.";
    }
}

fn FredFinder(internal, doPrint) { //if doPrint = true, will print the answer, if false will return.
    if doPrint == true {
        print(internalFredFinder(internal));
    }
    if doPrint == false {
        internalFredFinder(internal);
    }
}
